---
permalink: /admin/config.yml
eleventyExcludeFromCollections: true
---
{% assign theme = "theme-grunn" %}
backend:
  name: git-gateway
  branch: {{ env.branch }}
local_backend:
  # when using a custom proxy server port
  url: http://localhost:{{ env.netlifycmsport }}/api/v1
#SETTINGS
site_url: {{ env.url }}
logo_url: {{ env.url }}/assets/img/icon-admin.png
display_url: {{ env.url }}
search: true
# publish_mode: editorial_workflow
show_preview_links: true
slug:
  encoding: "ascii"
  clean_accents: true
  sanitize_replacement: "-"
media_folder: "src/content/upload/"
public_folder: "/content/upload"

