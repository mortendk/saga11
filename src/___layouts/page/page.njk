{%- if env.mode != 'prod' %}<!-- ___layouts/page/page.njk  -->{%- endif -%}

<article class="flex flex-col md:flex-row " aria-label="Page">
  <div class="md:w-2/3">

    <h1 class="font-serif text-8xl">{{ title }}</h1>



  {% include "gallery.njk" %}



    {% if image %}
      {% image image, [400,800, 1200],"(min-width: 30em) 50vw, 100vw",['webp'],alt,'','eager' %}
    {% endif %}

    <div class="prose prose-xl">
      {{ content | safe }}
    </div>

  </div>
  <div class="md:w-1/3">

  </div>
</article>


{% include "collection.njk" %}
